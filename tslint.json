{
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*",
            "dist/**/*"
        ]
    },
    "rules": {
        "no-console": false,
        "object-literal-sort-keys": false,
        "member-ordering": false
    },
    "jsRules": {
        "no-console": false
    }
}